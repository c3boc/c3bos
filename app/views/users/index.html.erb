<div class="col-md-12">
  <table class="table table-striped">
    <tr>
      <th>User</th>
      <th>Admin</th>
      <th>Actions</th>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user["username"] %></td>
        <td>+</td>
        <td>Edit, <%= link_to "Delete", user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %>, Admin</td>
      </tr>
    <% end %>
  </table>
</div>
